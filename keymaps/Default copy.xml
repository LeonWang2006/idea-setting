<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt equals" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="Console.Jdbc.Execute.Selection">
    <keyboard-shortcut first-keystroke="shift q" />
  </action>
  <action id="Debugger.PopFrame">
    <keyboard-shortcut first-keystroke="alt f5" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>